{
  "name": "okpt/my-web-page",
  "description": "Webpage von Kevin Piam",
  "keywords": ["okpt","importer"],
  "type": "project",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Kevin Piam",
      "email": "kevinpiam3@yahoo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "okpt\\my\\web\\page\\": "src/"
    },
    "autoload-dev": {
      "psr-4": {
        "App\\Tests\\": "tests/"
      }
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-memcached": "*",
    "ext-openssl": "*",
    "api-platform/core": "^2.6",
    "beberlei/doctrineextensions": "^1.3",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/annotations": "^1.0",
    "doctrine/dbal": "^3.3",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "dompdf/dompdf": "^2.",
    "easycorp/easyadmin-bundle": "^4.",
    "endroid/qr-code-bundle": "^4.",
    "friendsofsymfony/elastica-bundle": "^6.",
    "guzzlehttp/guzzle": "^7.4",
    "jms/serializer-bundle": "^5.",
    "keyvanakbary/slugifier": "^4.",
    "knplabs/doctrine-behaviors": "^2.",
    "myclabs/deep-copy": "^1.11",
    "nelmio/cors-bundle": "^2.2",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "sensio/framework-extra-bundle": "^6.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^6.0.",
    "symfony/config": "^5.0",
    "symfony/console": "^6.0.",
    "symfony/dependency-injection": "^5.0.0",
    "symfony/dotenv": "^6.0.",
    "symfony/expression-language": "^6.0.",
    "symfony/filesystem": "^6.0.",
    "symfony/flex": "^2",
    "symfony/form": "^6.0.",
    "symfony/framework-bundle": "^6.0.",
    "symfony/http-client": "^6.0.",
    "symfony/http-foundation": "^6.0.",
    "symfony/http-kernel": "^5.0",
    "symfony/intl": "^6.0.",
    "symfony/lock": "^6.0.",
    "symfony/mailer": "^6.0.",
    "symfony/mailgun-mailer": "6.0.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/process": "^6.0.",
    "symfony/property-access": "^6.0.",
    "symfony/property-info": "^6.0.",
    "symfony/proxy-manager-bridge": "^6.0.",
    "symfony/runtime": "^6.0.",
    "symfony/security-bundle": "^6.0.",
    "symfony/serializer": "^6.0.",
    "symfony/templating": "^6.0.",
    "symfony/translation": "^6.0.",
    "symfony/twig-bundle": "^6.0.",
    "symfony/uid": "^6.1.",
    "symfony/validator": "^6.0.",
    "symfony/web-link": "^6.0.",
    "symfony/yaml": "^6.0.",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "^2."
  },
  "provide": {
    "psr/log-implementation": "3.0.0"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "gitlab-domains": [ "git.thm.de" ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "check": "vendor/bin/phpcs src/",
    "fix": "vendor/bin/phpcbf src/"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.0.*"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8.6",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/maker-bundle": "^1.38"
  }
}
